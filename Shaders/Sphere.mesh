	vec4(1.0,1.0,1.0,0.0),
	vec4(0.0,1.0,1.0,0.0),
};
vec4 gridRotated[12] = 
{
	vec4(0.5,0.0,0.0,1.0),
	vec4(1.0,0.5,0.0,1.0),
	vec4(0.5,1.0,0.0,1.0),
	vec4(0.0,0.5,0.0,1.0),

	vec4(0.5,0.0,1.0,1.0),
	vec4(1.0,0.5,1.0,1.0),
	vec4(0.5,1.0,1.0,1.0),
	vec4(0.0,0.5,1.0,1.0),

	vec4(0.0,0.0,0.5,1.0),
	vec4(1.0,0.0,0.5,1.0),
	vec4(1.0,1.0,0.5,1.0),
	vec4(0.0,1.0,0.5,1.0),
};

void main()
{
	vec3 spherePos = vec3(5.0,-5.0,5.0);

	float scale = 0.25f;

    vec3 globPos = vec3(gl_GlobalInvocationID.x,gl_GlobalInvocationID.y,gl_GlobalInvocationID.z);
	vec4 offset = vec4(scale*gl_GlobalInvocationID.x, scale*gl_GlobalInvocationID.y, scale*gl_GlobalInvocationID.z, 0.0);

	vec4 vecScale = vec4(scale,scale,scale,1.0);

    int bitResult = 0;

    for(int i = 0; i < 8; i++)
    {
		float dist = distance(offset.xyz + scale*(grid[i].xyz - vec3(15.0,15.0,15.0)),spherePos);
        dist += sin(consts.g_extraConsts[0])*0.7;
		if(dist >= 9.0 && dist <= 10.0)
        {
            bitResult |= 1 << i;
        }
    }


	offset.x -= 15.0;

	for(int i = 0; i < 12; i++)
	{
		gl_MeshVerticesEXT[i].gl_Position = compute_position(gridRotated[i]*vecScale + offset);
		vertexOutput[i].color = colors[i % 4];
	}

		//gl_PrimitiveTriangleIndicesEXT[gl_LocalInvocationIndex + i] = uvec3(i, (i + 3) & 0x03, i + 4);
		//gl_PrimitiveTriangleIndicesEXT[gl_LocalInvocationIndex + i + 4] =	uvec3(i + 8, ((i + 3) & 0x03) + 8, i + 4);

	int outInd = 0;
    for(int i = 0,j = 0;; i += 3, j += 1)
    {
        if(triTable[bitResult][i] == -1)
            break;

    	gl_PrimitiveTriangleIndicesEXT[j] = uvec3(triTable[bitResult][i],triTable[bitResult][i+1],triTable[bitResult][i+2]);
        outInd = j + 1;
    }

	SetMeshOutputsEXT(12, outInd);
}
